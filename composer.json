{
    "name": "bugloos/laravel-localization",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bugloos\\LaravelLocalization\\": "src/",
            "Bugloos\\LaravelLocalization\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "mohammad.m",
            "email": "mohammad.m@bugloos.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpstan": "^1.9",
        "vimeo/psalm": "^5.4",
        "psalm/plugin-laravel": "^2.7",
        "symfony/finder": "^6.0",
        "phpstan/phpstan-php-parser": "^1.1",
        "phpstan/phpdoc-parser": "^1.15",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
